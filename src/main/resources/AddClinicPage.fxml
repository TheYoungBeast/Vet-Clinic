<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<?import javafx.scene.image.ImageView?>
<?import javafx.scene.image.Image?>

<?import jdk.internal.joptsimple.internal.Columns?>
<?import javafx.geometry.Insets?>
<AnchorPane xmlns="http://javafx.com/javafx" xmlns:fx="http://javafx.com/fxml" fx:controller="app.AddClinicFxController" prefHeight="400.0" prefWidth="600.0">
<children>
    <BorderPane layoutX="-25.0" layoutY="-32.0" prefHeight="400.0" prefWidth="600.0" style="-fx-background-color: #f1f1f3;">
    <right>
        <ImageView fitHeight="406.0" fitWidth="267.0" pickOnBounds="true" preserveRatio="true" BorderPane.alignment="CENTER">
            <image>
                <Image url="@dogtor.png" />
            </image>
        </ImageView>
    </right>
        <left>
            <VBox prefHeight="207.0" prefWidth="476.0">
                <children>
                <TableView fx:id="clinicsTableView" prefHeight="168.0" prefWidth="476.0" style="-fx-alignment: CENTER;">
                    <columns>
                        <TableColumn fx:id="clinicNameCol" prefWidth="75.0" text="Clinic Name" />
                        <TableColumn fx:id="clinicPhoneNumberCol" prefWidth="75.0" text="Contact Number" />
                        <TableColumn fx:id="clinicZipCodeCol" prefWidth="75.0"  text="Zip-Code" />
                        <TableColumn fx:id="clinicCityCol" prefWidth="75.0" text="City" />
                        <TableColumn fx:id="clinicTypeCol" prefWidth="75.0" text="Clinic Type" />
                    </columns>
                <columnResizePolicy>
                        <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
                    </columnResizePolicy>
                </TableView>
                </children>
                <VBox.margin>
                    <Insets top="100.0" />
                </VBox.margin>
            </VBox>
        </left>
    </BorderPane>
</children>
</AnchorPane>
